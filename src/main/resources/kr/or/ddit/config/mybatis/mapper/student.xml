<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="student">

	<select id="getStdId" resultType="studentVo" parameterType="string">
		SELECT *
		  FROM student
		 WHERE std_id = #{std_id}
	</select> 
  
</mapper>